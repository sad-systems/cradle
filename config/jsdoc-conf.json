{
    "project" : {
        "name":"Cradle",
      "number":"1.0.0",
       "brief":"Simple library for creating Web-based applications",
        "logo":""
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
    "include": [ "src" ],
    "exclude": [],
    "includePattern": ".+\\.(js|h)$",
    "excludePattern": ""
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
    "template": "templates/jsdoc",
    "encoding": "utf8",            
    "destination": "doc/jsdoc",
    "recurse": true
    }
}
